@{
    ViewBag.Title = "Home Page";
}


<div id="myCarousel" class="carousel slide" data-ride="carousel">
        <ol class="carousel-indicators">
            <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
            <li data-target="#myCarousel" data-slide-to="1" class=""></li>
            <li data-target="#myCarousel" data-slide-to="2" class=""></li>
        </ol>
        <div class="carousel-inner">
            <div class="item active">
                <img class="d-block w-100" src="~/Content/Images/Canada1.jpeg" alt="First slide">

            </div>
            <div class="item">
                <img class="d-block w-100" src="/Content/Images/Canada2.jpeg" alt="Second slide">

            </div>
            <div class="item">
                <img class="d-block w-100" src="/Content/Images/Canada.jpeg" alt="Third slide">

            </div>
        </div>
        <a class="left carousel-control" href="#myCarousel" data-slide="prev">
            <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
        </a>
        <a class="right carousel-control" href="#myCarousel"  data-slide="next">
            <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
        </a>
</div>



